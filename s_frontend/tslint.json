{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier",
    "tslint-eslint-rules",
    "tslint-consistent-codestyle"
  ],
  "jsRules": {
    "semicolon": [true, "never"] /* Semicolons are not free, remove them */
  },
  "rules": {
    "no-console": [false],
	"semicolon": [true, "never"] /* Semicolons are not free, remove them */
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}
